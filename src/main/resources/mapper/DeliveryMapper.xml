<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.edu.scau.linyuanbin.recruitment.mapper.DeliveryMapper">
    <resultMap id="deliveryMap" type="Delivery">
        <id column="deliveryId" property="deliveryId"></id>
        <result column="positionId" property="positionId"></result>
        <result column="personId" property="personId"></result>
        <result column="deliveryTime" property="deliverytime"></result>
        <result column="status" property="status"></result>

        <association property="position" column="positionId" select=""></association>
        <association property="resume" column="personId" select=""></association>
        <association property="person" column="personId" select=""></association>
        <association property="feedBack" column="deliveryId" select=""></association>
<!--        <collection property="" column="" select=""></collection>-->
    </resultMap>
</mapper>